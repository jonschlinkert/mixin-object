    "prop",
    "properties",
    "property",
    "shallow",
    "util",
    "value"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "related": {
      "list": [
        "assign-deep",
        "defaults-deep",
        "extend-shallow",
        "for-in",
        "for-own",
        "is-plain-object",
        "isobject",
        "merge-deep",
        "mixin-deep"
      ]
    },
    "plugins": [
      "gulp-format-md"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
